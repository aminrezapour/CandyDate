<div class="row", id="amin9">
    <div class="col-xs-5" id="amin7">

          <div class="pull-right" id="amin6">
            <%= image_tag @user.image, size: "60x60" %>
          </div>

    </div>

    <div class="col-xs-7" id="amin7">

          <div class="pull-left" id="amin8">
            <h2 class="media-heading"><b><%= @user.name %></b></h2>

            <% if @user.telephone.nil? %>
                <%= form_for @user do |f| %>
                     <div class="form-group">
                       <%= f.text_field :telephone, class: 'form-control', placeholder: "enter your phone" %>
                     </div>
                     <div class="form-group">
                       <%= f.submit "Save", class: 'btn btn-success' %>
                     </div>
                <% end %>
            <% else %>
                  <h4 class="media-heading"><%= @user.telephone %></h4>
            <% end %>

        </div>

    </div>

</div>


  <% unless @user.telephone.nil? %>

          <div id="amin10">
            <%= form_tag("/find_user", method: :post) do %>
                <%= text_field_tag(:tel, nil, placeholder: "phone number, e.g. 2132715224", class: "form-control") %>
                <%= submit_tag("Make a new date", class: "btn btn-block", id: "amin9") %>
            <% end %>
          </div>

  <% end %>




