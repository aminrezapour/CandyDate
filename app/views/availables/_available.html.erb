<div class="row">
  <%= link_to(edit_user_available_path(current_user, available)) do %>
    <div class="col-xs-10" id="amin8">
      <h3><%= available.slot.strftime("%A %b, %e at %l%P") %></h3>
    </div>
  <% end %>
  <%= link_to [current_user, available], method: :delete, data: {confirm: 'Are you sure?'} do %>
    <div class="col-xs-2" id="amin11">
      <h5>Delete</h5>
    </div>
  <% end %>
</div>
